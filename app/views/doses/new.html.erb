<div class="container">
  <h1>New cocktail dose</h1>

  <%= simple_form_for([@cocktail, @dose]) do |f| %>
    <%= f.error_notification %>
    <%= f.input :description %>
    <%= f.association :ingredient %>
    <%= f.submit class: "btn btn-primary" %>
  <% end %>

  <div><%= link_to 'Back', cocktail_path(@cocktail) %> </div>

</div>
